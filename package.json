{
  "name": "node-react-typescript-client-side-boilerplate",
  "version": "1.0.0",
  "description": "node server react typescript boilerplate with client side",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antonkalik/node-react-typescript-client-side-boilerplate.git"
  },
  "author": "antonkalik@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "npm run prod:build && node dist/server_bundle.js",
    "prod:build": "concurrently \"npm run prod:server\" \"npm run prod:client\"",
    "prod:server": "NODE_ENV=production webpack --config ./webpack.server.js --mode production",
    "prod:client": "NODE_ENV=production webpack --config ./webpack.client.js --mode production",
    "test": "jest --config ./jest.config.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "NODE_ENV=development nodemon",
    "dev:client": "NODE_ENV=development webpack-dev-server --config ./webpack.client.js",
    "lint": "eslint ./src -c .eslintrc.json",
    "index": "node ./scripts/generate-indexes.js -all",
    "new": "node ./scripts/new.js",
    "del": "node ./scripts/delete.js",
    "update": "node scripts/update.js"
  },
  "main": "src/client/index.tsx",
  "bugs": {
    "url": "https://github.com/antonkalik/node-react-typescript-client-side-boilerplate/issues"
  },
  "homepage": "https://github.com/antonkalik/node-react-typescript-client-side-boilerplate#readme",
  "keywords": [
    "node",
    "react",
    "typescript",
    "client-side",
    "boilerplate"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/enzyme": "^3.10.5",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-jest": "^25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "webpack-merge": "^4.2.2"
  }
}
